<?xml version="1.0"?>
<launch>
    <node pkg="lidar_tracking" name="lidar_tracking_nnew" type="lidar_tracking_nnew">
        <param name="topic" value="laser_control"/>
        <!-- <param name="topic" value="car/cmd_vel"/> -->
        <param name="speed" value="1850"/>
        <param name="kp" value="0.8"/>
        <param name="ki" value="0.0"/>
        <param name="kd" value="1.25"/>
        <param name="max_left_dis" value="1.5"/>
        <param name="max_right_dis" value="1.5"/>

        <param name="rate1" value="0.8"/>
        <param name="rate2" value="2.2"/>
        <param name="rate3" value="0.75"/>
        <param name="middle_angle" value="84"/>
        
        <param name="vaild_bucket_threshold" value="2"/>
        <param name="bucket_threshold" value="5"/>

    </node>

</launch>

